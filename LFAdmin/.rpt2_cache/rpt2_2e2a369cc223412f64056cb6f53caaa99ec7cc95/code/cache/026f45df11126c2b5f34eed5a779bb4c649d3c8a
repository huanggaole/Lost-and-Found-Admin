  "code": "export default class GameScene extends Laya.Scene {\r\n    createChildren() {\r\n        super.createChildren();\r\n        this.loadScene(\"GameScene.scene\");\r\n    }\r\n    onMouseDown() {\r\n        if (this.selectedSprite != null) {\r\n            this.ifCanmove = true;\r\n            this.lastx = Laya.stage.mouseX;\r\n            this.lasty = Laya.stage.mouseY;\r\n        }\r\n    }\r\n    onAwake() {\r\n        this.on(Laya.Event.MOUSE_DOWN, this, this.onMouseDown);\r\n        this.s1.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s1; });\r\n        this.s2.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s2; });\r\n        this.s3.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s3; });\r\n        this.s4.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s4; });\r\n        this.s5.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s5; });\r\n        this.s6.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s6; });\r\n        this.s7.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s7; });\r\n        this.s8.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s8; });\r\n        this.s9.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s9; });\r\n        this.s10.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s10; });\r\n        this.s11.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s11; });\r\n        this.s12.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s12; });\r\n        this.s13.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s13; });\r\n        this.s14.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s14; });\r\n        this.s15.on(Laya.Event.MOUSE_DOWN, this, () => { this.selectedSprite = this.s15; });\r\n        this.on(Laya.Event.MOUSE_MOVE, this, () => {\r\n            if (this.ifCanmove) {\r\n                let dx = Laya.stage.mouseX - this.lastx;\r\n                let dy = Laya.stage.mouseY - this.lasty;\r\n                this.lastx = Laya.stage.mouseX;\r\n                this.lasty = Laya.stage.mouseY;\r\n                let rigidbody = this.selectedSprite.getComponent(Laya.RigidBody);\r\n                rigidbody.setVelocity({ x: dx, y: dy });\r\n            }\r\n            if (this.target.getBounds().intersects(this.s1.getBounds())) {\r\n                this.s1.destroy();\r\n            }\r\n        });\r\n        this.on(Laya.Event.MOUSE_UP, this, () => {\r\n            this.ifCanmove = false;\r\n            let rigidbody = this.selectedSprite.getComponent(Laya.RigidBody);\r\n            rigidbody.setVelocity({ x: 0, y: 0 });\r\n            this.selectedSprite = null;\r\n        });\r\n    }\r\n}\r\n",
